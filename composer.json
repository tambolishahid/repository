{
  "name": "reotrix/repositories",
  "type": "library",
  "description": "Repository pattern implementation for Laravel5",
  "license": "MIT",
  "homepage": "http://www.reotrix.com",
  "keywords": ["repository","laravel"],
  "authors": [
    {
      "name": "Shahid Tamboli",
      "email": "tambolishahid@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/cache": "~5.2.0|~5.3.0",
    "illuminate/config": "~5.2.0|~5.3.0",
    "illuminate/support": "~5.2.0|~5.3.0",
    "illuminate/database": "~5.2.0|~5.3.0",
    "illuminate/filesystem": "~5.2.0|~5.3.0",
    "illuminate/console": "~5.2.0|~5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "orchestra/testbench": "3.2.x-dev",
    "orchestra/database": "3.2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Reotrix\\Repositories\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Reotrix\\Repositories\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
